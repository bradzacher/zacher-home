{
  "name": "zacher-home",
  "version": "1.0.0",
  "author": "Brad Zacher",
  "description": "the landing page of zacher.com.au",
  "license": "UNLICENCED",
  "scripts": {
    "prestart": "yarn clean",
    "start": "parcel ./src/index.njk",
    "fetch-extras": "yarn ts-node --files ./scripts/github.ts && yarn ts-node --files ./scripts/wakatime.ts",
    "prebuild": "yarn clean && yarn fetch-extras",
    "build": "yarn clean && yarn parcel build ./src/index.njk",
    "postbuild": "yarn cpx ./src/assets/me.jpg ./dist/assets/",
    "clean": "rimraf dist .cache",
    "lint": "eslint src --ext .ts"
  },
  "devDependencies": {
    "@types/chart.js": "^2.7.25",
    "@types/jsdom": "^11.0.6",
    "chart.js": "^2.2.2",
    "cpx": "^1.5.0",
    "eslint-config-assignar-node": "^0.1.5",
    "eslint-import-resolver-node": "^0.3.2",
    "github-calendar": "^1.1.10",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^11.11.0",
    "material-design-icons": "^3.0.1",
    "material-design-lite": "^1.2.0",
    "node-sass": "^4.9.0",
    "nunjucks": "^3.1.3",
    "parcel": "^1.9.3",
    "parcel-bundler": "^1.9.3",
    "parcel-plugin-nunjucks": "^1.0.0",
    "parcel-plugin-sw-precache": "^0.5.1",
    "parcel-plugin-typescript": "^1.0.0",
    "parcel-plugin-url-loader": "^1.2.5",
    "rimraf": "^2.6.2",
    "ts-node": "^7.0.0",
    "typescript": "^2.9.2"
  }
}
