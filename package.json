{
  "name": "zacher-home",
  "version": "2.0.0",
  "author": "Brad Zacher",
  "description": "the landing page of zacher.com.au",
  "license": "UNLICENCED",
  "scripts": {
    "generate": "yarn run-script ./scripts/generate/generate.ts",
    "build": "yarn run-script ./scripts/buildReact.ts",
    "clean": "yarn run-script ./scripts/clean.ts",
    "copy-assets": "cpx ./src/assets/**/* ./build",
    "format-no-write": "prettier 'src/**/*.{ts,tsx}' 'scripts/**/*.{ts,tsx}' 'typings/**/*.{ts,tsx}' --config=.prettierrc",
    "format": "yarn format-no-write --write && yarn lint --fix",
    "format-check": "yarn format-no-write --list-different",
    "lint": "eslint src scripts typings --ext=.ts,.tsx",
    "lint-errors": "yarn lint --quiet",
    "run-script": "ts-node -T -P ./scripts/tsconfig.json ./scripts/scriptExecutor.ts",
    "prestart": "yarn generate && yarn build",
    "start": "yarn run-script ./scripts/server.ts",
    "typecheck": "tsc -p ./tsconfig.json --noEmit && tsc -p ./scripts/tsconfig.json --noEmit"
  },
  "devDependencies": {
    "@types/browser-sync": "^2.26.3",
    "@types/chart.js": "^2.9.37",
    "@types/chokidar": "^2.1.3",
    "@types/clean-css": "^4.2.5",
    "@types/cpx": "^1.5.2",
    "@types/globby": "^9.1.0",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^18.7.9",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/rimraf": "^3.0.2",
    "@types/xml2js": "^0.4.11",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "aws-sdk": "^2.1199.0",
    "browser-sync": "^2.27.10",
    "chart.js": "^3.9.1",
    "chokidar": "^3.5.3",
    "classnames": "^2.3.1",
    "clean-css": "^5.3.1",
    "cpx": "^1.5.0",
    "eslint": "^8.22.0",
    "eslint-config-brad": "^5.0.9",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.7",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-restricted-globals": "0.2.0",
    "globby": "^11.0.4",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^28.1.3",
    "jsdom": "^20.0.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-jss": "^10.9.2",
    "rimraf": "^3.0.2",
    "schema-dts": "^1.1.0",
    "spritesmith": "^3.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "xml2js": "^0.4.23"
  },
  "resolutions": {
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@types/node": "^18.7.9",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}
