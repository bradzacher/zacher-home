{
    "name": "zacher-home",
    "version": "2.0.0",
    "author": "Brad Zacher",
    "description": "the landing page of zacher.com.au",
    "license": "UNLICENCED",
    "scripts": {
        "generate": "yarn run-script ./scripts/generate.ts",
        "build": "yarn run-script ./scripts/buildReact.ts",
        "clean": "yarn run-script ./scripts/clean.ts",
        "copy-assets": "cpx ./src/assets/**/* ./build",
        "format-no-write": "prettier 'src/**/*.{ts,tsx}' 'config/**/*.{ts,tsx}' 'scripts/**/*.{ts,tsx}' 'typings/**/*.{ts,tsx}' --config=.prettierrc",
        "format": "yarn format-no-write --write && yarn lint --fix",
        "format-check": "yarn format-no-write --list-different",
        "lint": "eslint src scripts typings --ext=.ts,.tsx",
        "lint-errors": "yarn lint --quiet",
        "run-script": "ts-node --files ./scripts/scriptExecutor.ts",
        "prestart": "yarn generate && yarn build",
        "start": "yarn run-script ./scripts/server.ts",
        "typecheck": "tsc --noEmit"
    },
    "devDependencies": {
        "@types/browser-sync": "^2.26.0",
        "@types/chart.js": "^2.7.55",
        "@types/chokidar": "^2.1.3",
        "@types/classnames": "^2.2.9",
        "@types/clean-css": "^4.2.1",
        "@types/cpx": "^1.5.0",
        "@types/globby": "^9.1.0",
        "@types/jsdom": "^12.2.4",
        "@types/react": "^16.8.23",
        "@types/react-dom": "^16.8.4",
        "@types/react-jss": "^8.6.3",
        "@types/rimraf": "^2.0.2",
        "@types/xml2js": "^0.4.4",
        "aws-sdk": "^2.489.0",
        "browser-sync": "^2.26.7",
        "chart.js": "^2.8.0",
        "chokidar": "^3.0.2",
        "classnames": "^2.2.6",
        "clean-css": "^4.2.1",
        "cpx": "^1.5.0",
        "eslint-config-brad": "^1.6.5",
        "eslint-import-resolver-typescript": "^1.1.1",
        "globby": "^10.0.1",
        "isomorphic-fetch": "^2.2.1",
        "jsdom": "^15.1.1",
        "prettier": "^1.18.2",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-jss": "^8.6.1",
        "rimraf": "^2.6.3",
        "schema-dts": "^0.4.1",
        "spritesmith": "^3.3.1",
        "ts-node": "^8.3.0",
        "typescript": "^3.5.3",
        "xml2js": "^0.4.19"
    },
    "resolutions": {
        "@types/classnames": "2.2.9",
        "@types/react": "16.8.23",
        "@types/react-dom": "16.8.4",
        "@types/react-jss": "8.6.3",
        "react": "16.8.6",
        "react-dom": "16.8.6"
    }
}
