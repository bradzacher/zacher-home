{
    "extends": [
        "brad/react"
    ],
    "parserOptions": {
        "project": "./tsconfig.json"
    },
    "rules": {
        "react/jsx-no-target-blank": "off"
    },
    "settings": {
        "import/resolver": {
            "typescript": {}
        },
        "react": {
            "version": "16.6.3"
        }
    },
    "overrides": [
        {
            "files": ["scripts/**/*"],
            "rules": {
                "import/no-default-export": "off"
            }
        }
    ]
}
