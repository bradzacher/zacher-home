{% from "./macros/project.njk" import project %}

{% set heading = "Open Source Contributions" %}
{% set clazz = "projects-card" %}
{% extends "./macros/card.njk" %}

{% block body %}
<div class="mdl-grid">

    {{ project(
        "mysqldump",
        "mysqldump",
        "Node Module to Create a Backup from MySQL.",
        source = "https://github.com/assignar/mysqldump",
        demo = "https://www.npmjs.com/package/@assignar/mysqldump"
    ) }}

    {{ project(
        "DefinitelyTyped",
        "DefinitelyTyped",
        "TypeScript type definitions for node modules.",
        source = "https://github.com/DefinitelyTyped/DefinitelyTyped"
    ) }}

    {{ project(
        "assignar",
        "eslint-config-assignar",
        "Assignar's ESlint configuration for typescript projects.",
        source = "https://github.com/assignar/eslint-config-assignar"
    ) }}

    {{ project(
        "eslint",
        "eslint-plugin-typescript",
        "Additional ESlint rules for Typescript code.",
        source = "https://github.com/nzakas/eslint-plugin-typescript",
        demo = "https://www.npmjs.com/package/eslint-plugin-typescript"
    ) }}

</div>
{% endblock %}
